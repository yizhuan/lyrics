{
  "name": "lyricsapp",
  "version": "0.0.1",
  "author": "Ionic Framework + JHipster",
  "homepage": "https://www.jhipster.tech",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e --port 8100",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write \"{,e2e/**/,src/**/}*.{js,json,html,md,ts,css,scss,yml}\" --loglevel silent"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/forms": "~9.1.6",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "rxjs": "~6.5.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "@ionic-native/camera": "5.25.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "ngx-webstorage": "5.0.0",
    "ng-jhipster": "0.13.0",
    "@fortawesome/angular-fontawesome": "0.6.1",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@ng-bootstrap/ng-bootstrap": "6.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "~9.1.5",
    "@angular/compiler": "~9.1.6",
    "@angular/compiler-cli": "~9.1.6",
    "@angular/language-service": "~9.1.6",
    "@ionic/angular-toolkit": "^2.1.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "generator-jhipster": "6.8.0",
    "@types/jest": "25.2.2",
    "html-loader": "1.1.0",
    "ionic-mocks-jest": "1.3.3",
    "istanbul-instrumenter-loader": "3.0.1",
    "jest": "26.0.1",
    "jest-localstorage-mock": "2.4.2",
    "jest-preset-angular": "8.2.0",
    "null-loader": "4.0.0",
    "prettier": "2.0.5",
    "ts-loader": "7.0.4",
    "tslint-config-prettier": "1.18.0",
    "generator-jhipster-ionic": "5.2.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there"
      }
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|@ionic-native|@ionic)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.{js,ts}",
      "!src/app/**/index.ts",
      "!src/app/**/*.module.ts",
      "!<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "./public/coverage"
  },
  "description": "A hipster Ionic project, made with ðŸ’™ by @oktadev!"
}
